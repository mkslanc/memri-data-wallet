.inbox {
    name: "inbox"
    title: "Inbox"
    defaultRenderer: list
    cols: 4
          
    [datasource = pod] {
        query: MessageChannel
        sort: {
            targetType: "Message"
            edgeSource: {
                name: messageChannel
                sortProperty: dateSent
                sortAscending: false
            }
        }
    }
}
